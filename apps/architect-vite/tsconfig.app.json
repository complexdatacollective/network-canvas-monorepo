{
	"extends": "@codaco/tsconfig/base.json",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"~/*": ["./*"]
		},
		"tsBuildInfoFile": "node_modules/.cache/tsbuildinfo.json"
	},
	"include": ["**/*.js", "**/*.jsx", "**/*.mjs", "**/*.cjs", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
	"exclude": ["node_modules"]
}
