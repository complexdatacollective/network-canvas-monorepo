{
	"extends": "@codaco/tsconfig/base.json",
	"compilerOptions": {
		"composite": true,
		"baseUrl": ".",
		"paths": {
			"~/*": ["./*"]
		},
		"plugins": [
			{
				"name": "next"
			}
		],
		"tsBuildInfoFile": "node_modules/.cache/tsbuildinfo.json"
	},
	"include": ["**/*.cjs", "**/*.js", "**/*.jsx", "**/*.mjs", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
	"exclude": ["node_modules"]
}
