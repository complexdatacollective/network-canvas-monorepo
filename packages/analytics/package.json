{
	"name": "@codaco/analytics",
	"version": "8.0.0",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"author": "Complex Data Collective <hello@complexdatacollective.org>",
	"description": "Utilities for tracking analytics and error reporting in Fresco",
	"scripts": {
		"build": "tsup src/index.ts --format esm --dts --clean --sourcemap",
		"publish": "pnpm build && npm publish --access public",
		"dev": "npm run build -- --watch",
		"test": "vitest run",
		"test:watch": "vitest",
		"typecheck": "tsc --noEmit"
	},
	"peerDependencies": {
		"next": "catalog:"
	},
	"devDependencies": {
		"@codaco/tsconfig": "workspace:*",
		"@types/node": "catalog:",
		"tsup": "^8.5.1",
		"typescript": "catalog:",
		"vitest": "catalog:"
	},
	"dependencies": {
		"@codaco/shared-consts": "workspace:*",
		"zod": "catalog:"
	}
}
